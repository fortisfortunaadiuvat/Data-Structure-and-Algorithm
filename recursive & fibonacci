#include <stdio.h>
#include <stdlib.h>
	
void yaz(int test){
	if (test < 1)
		return;
	else{
		printf("%d ",test);
	yaz(test-1);
	printf("%d ",test);
	return;
	}
}

int main(){
	int test = 3;
	yaz(test);
  
  return 0;
}

#include<stdio.h>

int fib(int n){
  if (n <= 1)
  return n;
  return fib(n-1) + fib(n-2);
}

int main (){
  int n = 4;
  printf("%d", fib(n));
  getchar();
  return 0;
}
